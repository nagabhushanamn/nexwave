package com.mts;

import com.mts.service.TxrService;
import com.mts.service.TxrServiceImpl;

public class App {

	public static void main(String[] args) {

		// Init
		TxrService txrService = new TxrServiceImpl();

		// Use
		txrService.txr(100.00, "1", "2");

		// Destroy
		txrService = null;
	}

}
