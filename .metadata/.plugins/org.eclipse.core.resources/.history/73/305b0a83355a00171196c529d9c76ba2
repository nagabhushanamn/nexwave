	<div class="page-header">JSP Learn</div>
	
	<%-- <%@include file="banner.html" %> --%>

	<%
		Person p = (Person) request.getAttribute("person");
	%>
	
	<!-- HTML comment <%=1+2 %> -->
	<%-- JSP comment  <%=1+2 %> <h1>Hello</h1> --%>
	
	<pre>
		Id   : <%=p.getId() %>
		Name : <%=p.getName() %>
		Age  : <%=person.getAge() %>
	</pre>

	<jsp:useBean id="person" class="com.app.model.Person" scope="request">
	</jsp:useBean>
	<pre>
		Id   : <jsp:
		Name : <%=person.getName() %>
		Age  : <%=person.getAge() %>
	</pre>



</body>
