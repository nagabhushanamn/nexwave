	<%
		Person person = (Person) request.getAttribute("person");
	%>
	
	<!-- HTML comment <%=1+2 %> -->
	<%-- JSP comment  <%=1+2 %> <h1>Hello</h1> --%>
	
	<pre>
		Id   : <%=person.getId() %>
		Name : <%=person.getName() %>
		Age  : <%=person.getAge() %>
	</pre>




